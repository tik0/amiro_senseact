<?xml version="1.0" encoding="UTF-8"?>
<ChainSwitchConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="ChainSwitchConfiguration.xsd">

        <Tunnels>
                <Tunnel name="trigger_kinect" from="start" to="KinectTrigger" type="NOTHING"/>
                <Tunnel name="trigger_5d" from="start" to="trigger_5d" type="NOTHING"/>
                <Tunnel name="pca_to_cold" from="pca_boxes_out" to="coldBoundingBoxes_in" type="STRING"/>
                <Tunnel name="cold_to_pmk" from="ColdBBoxOut" to="pmk_bb_in" type="STRING"/>
                <Tunnel name="cold_to_pmk-2" from="coldBoundingBoxes_out" to="pmk_bb_in" type="STRING"/>
                <Tunnel name="cold_to_export" from="ColdBBoxOut" to="export_bb_in" type="STRING"/>
                <Tunnel name="image_to_cold" from="image" to="cold_in" type="IMAGE"/>
                <Tunnel name="image_to_pmk" from="KinectRGB" to="PMKSVM_image_in" type="IMAGE"/>
                <!-- Tunnel name="pmk_to_export" from="PMKSVM_out" to="export_pmk_in" type="STRING"/>
                <Tunnel name="point_to_export" from="PMKSVM_point" to="export_point_in" type="STRING"/>
                <Tunnel name="clear" from="start" to="ClearTrigger" type="NOTHING"/-->
        </Tunnels>
        
        <Chains>
                <Chain name="odc">
                        <Item name="trigger_kinect"/>
                        <Item name="trigger_5d"/>
                        <Item name="pca_to_cold"/>
                        <Item name="image_to_cold"/>
                        <Item name="image_to_pmk"/>
                        <Item name="cold_to_pmk"/>
                        <Item name="cold_to_pmk-2"/>
                        <Item name="cold_to_export"/>
                        <!--Item name="pmk_to_export"/>
                        <Item name="point_to_export"/-->
                </Chain>
                <!-- Chain name="clearchain">
                        <Item name="clear"/>
                </Chain-->
        </Chains>

</ChainSwitchConfiguration>
