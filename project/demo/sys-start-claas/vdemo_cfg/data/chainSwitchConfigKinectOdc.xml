<?xml version="1.0" encoding="UTF-8"?>
<ChainSwitchConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ChainSwitchConfiguration.xsd">

	<Tunnels>
		<Tunnel name="trigger" from="start" to="KinectTrigger" type="NOTHING"/>
		<Tunnel name="cold_to_pmk" from="cold_out" to="PMKSVM_bb_in" type="STRING"/>
		<Tunnel name="cold2_to_pmk" from="coldBoundingBoxes_out" to="ColdBBoxOut" type="STRING"/>
		<Tunnel name="image_to_cold" from="KinectRGB" to="cold_in" type="IMAGE"/>
		<Tunnel name="image_to_pmk" from="KinectRGB" to="PMKSVM_image_in" type="IMAGE"/>
		<Tunnel name="pmk_to_export" from="PMKSVM_out" to="export_pmk_in" type="STRING"/>
		<Tunnel name="point_to_export" from="PMKSVM_point" to="export_point_in" type="STRING"/>
		<Tunnel name="clear" from="start" to="ClearTrigger" type="NOTHING"/>
	</Tunnels>
	
	<Chains>
		<Chain name="odc">
			<Item name="trigger"/>
			<Item name="image_to_cold"/>
			<Item name="image_to_pmk"/>
			<Item name="cold_to_pmk"/>
			<Item name="pmk_to_export"/>
			<Item name="point_to_export"/>
		</Chain>
		<Chain name="clearchain">
			<Item name="clear"/>
		</Chain>
	</Chains>

</ChainSwitchConfiguration>
